{
    "name": "qubus/valueobjects",
    "type": "library",
    "description": "A PHP library of immutable objects to use in Domain Driven Design.",
    "keywords": [
        "value-object",
        "valueobjects",
        "domain-driven-design",
        "ddd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Parker",
            "email": "joshua@joshuaparker.dev"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravie/geotools": "^1.2",
        "marc-mabe/php-enum": "^4",
        "moneyphp/money": "^4",
        "nesbot/carbon": "^3",
        "qubus/exception": "^3",
        "ramsey/uuid": "^4.3",
        "tuupola/base32": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1",
        "pestphp/pest": "^3.8",
        "qubus/qubus-coding-standard": "^1",
        "squizlabs/php_codesniffer": "^3",
        "symfony/var-dumper": "^5"
    },
    "autoload": {
        "psr-4": {
            "Qubus\\ValueObjects\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qubus\\Tests\\ValueObjects\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        }
    }
}
