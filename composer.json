{
    "name": "qubus/valueobjects",
    "type": "library",
    "description": "A PHP library of immutable objects to use in Domain Driven Design.",
    "keywords": [
        "value-object",
        "valueobjects",
        "domain-driven-design",
        "ddd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Parker",
            "email": "joshua@joshuaparker.dev"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravie/geotools": "^1.2",
        "marc-mabe/php-enum": "^4.7",
        "moneyphp/money": "^4.0",
        "nesbot/carbon": "^2",
        "qubus/exception": "^3.0",
        "ramsey/uuid": "^4.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.0",
        "qubus/qubus-coding-standard": "^1.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/var-dumper": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Qubus\\ValueObjects\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qubus\\Tests\\ValueObjects\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
